{
    "LABEL": {
        "name": "title needs formatting",
        "color": "EEEEEE"
    },
    "CHECKS": {
        "regexp": "^(?:add|update|feat|fix|refactor): (.*) \\( ?https?://([^:]+|\\[[:0-9a-fA-F]+\\])(:\\d+)?/? ?\\)",
        "regexpFlags": "i",
        "ignoreLabels": [
            "dont-check-PRs-with-this-label"
        ]
    }
}
